{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development npm run wrangler",
    "dev:wrangler": "remix watch",
    "generate:css": "tailwindcss -i ./styles/app-tailwind.css -o app/styles.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "wrangler": "wrangler pages dev ./public",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@radix-ui/react-context-menu": "2.1.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@radix-ui/react-popover": "1.0.3",
    "@radix-ui/react-select": "1.2.0",
    "@radix-ui/react-slot": "^1.0.1",
    "@remix-run/cloudflare": "1.11.0",
    "@remix-run/cloudflare-pages": "1.11.0",
    "@remix-run/react": "1.11.0",
    "@supabase/auth-helpers-remix": "^0.1.3",
    "@supabase/supabase-js": "2.4.1",
    "@tailwindcss/container-queries": "^0.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "framer-motion": "8.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "1.11.0",
    "@remix-run/eslint-config": "1.11.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "8.32.0",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "wrangler": "2.8.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
